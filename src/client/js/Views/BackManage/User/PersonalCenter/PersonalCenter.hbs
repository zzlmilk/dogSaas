<div id="personal_center_view">

    <div class="basic_info">
        <div class="title">
            <h3>Basic Info</h3>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Status：</h4>
                </div>
                <div class="section">
                    <span>{{organization.checkStatus.status}}</span>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle" style="float: left">
                    <h4>Account：</h4>
                </div>
                <div class="section" style="float: left">
                    <span>{{organization.adminUser.email}}</span>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle" style="float: left">
                    <h4>Password：</h4>
                </div>
                <div class="section" style="float: left">
                    <a href="#changepassword" id="change_password" style="color:#3399ff;height:44px;line-height:44px;">Change Password</a>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle" style="float: left">
                    <h4>Registration Date：</h4>
                </div>
                <div class="section" style="float: left">
                    <span><span>{{organization.adminUser.created}}</span></span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="subtitle" style="float: left">
                    <h4>Director：</h4>
                </div>
                <div class="section" style="float: left">
                    <span><span>{{organization.contacts.name}}</span></span>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle" style="float: left">
                    <h4>Mobile Number：</h4>
                </div>
                <div class="section" style="float: left">
                    <span><span>{{organization.contacts.phone}}</span></span>
                </div>
            </div>
        </div>

    </div>
    <div class="basic_info">
        <div class="title">
            <h3>Agency</h3>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Quarantine Station：</h4>
                </div>
                <div class="section">
                    <span><span>{{organization.name}}</span></span>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Address：</h4>
                </div>
                <div class="section">
                    <span><span>{{organization.location.address}}</span></span>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Telephone Number：</h4>
                </div>
                <div class="section">
                    <span><span>{{organization.tel}}</span></span>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Available Services：</h4>
                </div>
                <div class="section">
                    <span><span>{{organization.serviceScope}}</span></span>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Business license：</h4>
                </div>
                <div class="section">
                    <!--<div id="license">-->
                    <!--<img  src="img/license.png" />-->
                    <!--</div>-->
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Veterinary Hospital Permits：</h4>
                </div>
                <div class="section">
                    <!--<div id="permit">-->
                    <!--<img src="img/peimit.png" />-->
                    <!--</div>-->
                </div>
            </div>
        </div>
        <div class="row" style="height:180px;">
            <div class="col-md-3">
                <div class="subtitle">
                    <div id="license">
                        <img  src="{{organization.businessLicense}}" />
                    </div>
                </div>
                <div class="section">
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle">
                    <div id="permit">
                        <img src="{{organization.animalMedicalLicense}}" />
                    </div>
                </div>
                <div class="section">
                </div>
            </div>
        </div>
    </div>
    <div class="basic_info">
        <div class="title">
            <h3>Hardware Info</h3>
        </div>
        <div class="row">
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Device ID：</h4>
                </div>
                <div class="section">
                    <span><span>a23492312g</span></span>
                </div>
            </div>
            <div class="col-md-3">
                <div class="subtitle">
                    <h4>Hardware Vision：</h4>
                </div>
                <div class="section">
                    <span><span>V1.0.1</span></span>
                </div>
            </div>
        </div>
    </div>
    <div class="basic_info">
        <div class="title">
            <h3>
                <span>Veterinarian Info</span>
                <button class="btn btnBlueE btn-block" id="btn_account">Add Accredited Veterinarian</button>
            </h3>

        </div>
        <div class="row">
            {{#each organization.veterinarians}}

                <input style="display: none" value="{{this}}">
                <div class="col-md-4">

                    <div class="subtitle">
                        <h4>Name：</h4>
                    </div>
                    <div class="section">
                        <span>{{this.name}}</span>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="subtitle">
                        <h4>Accredited Certificate NO：</h4>
                    </div>
                    <div class="section">
                        <span>{{this.code}}</span>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="subtitle" style="visibility: hidden">
                        <h4>
                            <a href="javascript:void (0)" class="delete" style="color:red;" value="{{this}}">删除</a>
                        </h4>
                    </div>

                </div>
            {{/each}}

        </div>
    </div>

    <div id="change_password_view"></div>

</div>